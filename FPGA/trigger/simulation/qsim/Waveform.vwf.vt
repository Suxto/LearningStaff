`timescale 1 ps/ 1 ps
module trigger_vlg_sample_tst(
	CLK,
	D,
	nCLR,
	sampler_tx
);
input  CLK;
input [7:0] D;
input  nCLR;
output sampler_tx;

reg sample;
time current_time;
always @(CLK or D or nCLR)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

module trigger_vlg_check_tst (
